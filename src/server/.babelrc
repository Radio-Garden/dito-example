{
  "presets": [
    ["@babel/typescript", { "allowDeclareFields": true }],
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "14"
        },
        "corejs": 3,
        "useBuiltIns": "usage",
        "debug": true
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "cwd": "packagejson",
        "alias": {
          "~": ".",
          "@": "./src/server"
        }
      }
    ],
    "dynamic-import-node",
    "transform-promise-to-bluebird"
  ],
  "comments": false
}
